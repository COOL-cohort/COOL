{
    "brithSelector":{
        "birthEvents":[
            {
                "filters":[
                    {
                        "fieldSchema":"prescribe",
                        "filterType":"Set",
                        "fieldValue": {
                            "acceptValue":["Medicine-A"],
                            "valueType":"AbsoluteValue"
                        }
                    }
                ],
                "frequency":2
            }
        ],
        "timeWindow": {
            "length":7,
            "slice":true,
            "unit":"DAY"
        }
    },

    "ageSelector":{
        "unit":"DAY",
        "min":1,
        "max":7,
        "interval":1
    },

    "cohortSelector":{
        "fieldSchema":"birthyear",
        "type":"RANGE",
        "min":1950,
        "max":1990,
        "interval":10
    },

    "valueSelector":{
        "filters":[
            {
                "fieldSchema":"labtest",
                "type":"SET",
                "fieldValue": {
                    "acceptValue":["Medicine-C"],
                    "valueType":"AbsoluteValue"
                }
            },
            {
                "fieldSchema":"value",
                "type":"RANGE",
                "fieldValue": {
                    "acceptValue":["45-100"],
                    "valueType":"RangeValue"
                }
            }
        ],
        "function":["min","max","average","sum"],
        "observedSchema":"id"
    },

    "dataSource":"health",
    "inputCohort":"loyal",
    "measure":"retention"
}
